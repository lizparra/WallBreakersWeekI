class Solution {
    public int[] sortArrayByParity(int[] A) {
        int[] newArr = new int[A.length];
        int beginning = 0;
        int end = A.length - 1;
        for(int i = 0; i < A.length; i++){
            if(A[i] % 2 == 0){
                newArr[beginning] = A[i];
                beginning++;
            } else {
                newArr[end] = A[i];
                end--;
            }
        }
        return newArr;
    }
}
